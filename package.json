{
  "name": "voca-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 7024",
    "build": "prisma generate && next build",
    "build:static": "BUILD_MODE=static next build",
    "postinstall": "prisma generate",
    "start": "next start -p 7024",
    "lint": "next lint",
    "cap:init": "cap init",
    "cap:sync": "cap sync",
    "cap:open:ios": "cap open ios",
    "cap:open:android": "cap open android",
    "build:ios": "bash scripts/build-ios.sh",
    "build:ios:ipa": "bash scripts/build-ios.sh --export-ipa",
    "build:android": "bash scripts/build-android.sh",
    "build:android:copy": "bash scripts/build-android.sh --copy-outputs",
    "build:all": "bash scripts/build-all.sh",
    "build:mobile": "bash scripts/build-all.sh",
    "icon:ios": "node scripts/generate-ios-icon.js",
    "icon:android": "node scripts/generate-android-icon.js",
    "icon:all": "node scripts/generate-ios-icon.js && node scripts/generate-android-icon.js",
    "version:update": "node scripts/update-build-version.js",
    "version:check": "node scripts/update-build-version.js --check",
    "prepare": "husky"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/preferences": "^7.0.2",
    "@prisma/client": "^5.22.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "framer-motion": "^11.15.0",
    "idb": "^8.0.3",
    "ipa-hangul": "^1.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.469.0",
    "next": "15.0.3",
    "prisma": "^5.22.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
